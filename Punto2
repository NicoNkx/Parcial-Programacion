#include <iostream>
using namespace std;
int main(){
	int nums,num,x=0,z=1,resul;
	int con=0;
	cout<<"Â¿Cuantos numeros desea ingresar?: ";cin>>nums;
	for(int i=0;i<nums;i++){
		x=0;
		z=1;
		cout<<"\n";
		cout<<i+1<<".) Ingrese un numero entero: ";cin>>num;
		for(int j=0;j<num;j++){
			//cout<<"\numFibonacci: "<<n1<<endl;  
			x+=z;
			//cout<<"\numFibonacci: "<<n2<<endl; 
			z+=x;
			
			if(num==x or num==z ){
				//cout<<"\num==> El numero ingresado es Fibonacci.";
				con++;
				if(con ==2){
					resul=num;
				}
	        }
		}	
	}
	cout<<"Segundo fibonacci: "<<resul;
}
